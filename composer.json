{
    "name": "basry/larabackup",
    "description": "Simple Laravel Database Backup Solution",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/basry/larabackup/issues",
        "source": "https://github.com/basry/larabackup"
    },
    "authors": [
        {
            "name": "Ashraf Basry",
            "email": "ashrafmn2008@gmail.com"
        }
    ],
    "require": {
        "ifsnop/mysqldump-php": "^2.7",
        "illuminate/console": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Basry\\Larabackup\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "laravel": {
            "providers": [
                "Basry\\Larabackup\\LarabackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
